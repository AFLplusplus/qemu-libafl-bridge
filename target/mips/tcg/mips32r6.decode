REMOVED             011111 ----- ----- ----------  100001   # SWLE
REMOVED             011111 ----- ----- ----------  100010   # SWRE

REMOVED             100010 ----- ----- ----------------     # LWL
REMOVED             100110 ----- ----- ----------------     # LWR
REMOVED             101010 ----- ----- ----------------     # SWL
REMOVED             101110 ----- ----- ----------------     # SWR

REMOVED             101111 ----- ----- ----------------     # CACHE
